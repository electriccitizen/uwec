<?php

/**
 * @file
 * Install, update and uninstall functions for the Citizen Profile Sync module.
 */

/**
 * Implements hook_install().
 */
function citizen_profile_sync_install() {
  \Drupal::messenger()->addStatus(__FUNCTION__);
}

/**
 * Implements hook_uninstall().
 */
function citizen_profile_sync_uninstall() {
  \Drupal::messenger()->addStatus(__FUNCTION__);
}
